<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Territory_Level__c</fullName>
    <description>Indicates the level of the territory in the hierarchy (e.g., One, Two, Three, Four) based on how many parent territories it has.</description>
    <externalId>false</externalId>
    <formula>If
 (
   ISBLANK(ParentTerritory2.Name),
   &quot;One&quot;,
   If
    ( 
     ISBLANK(ParentTerritory2.ParentTerritory2.Name),
     &quot;Two&quot;,
     If
      ( 
       ISBLANK(ParentTerritory2.ParentTerritory2.ParentTerritory2.Name),
       &quot;Three&quot;,
       &quot;Four&quot;
       )
     )
  )</formula>
    <label>Territory Level</label>
    <required>false</required>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
