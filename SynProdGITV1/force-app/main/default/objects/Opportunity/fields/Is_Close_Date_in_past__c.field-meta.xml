<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Close_Date_in_past__c</fullName>
    <description>INC 163276: To check if the close date is in the past for Component Visibility</description>
    <formula>CloseDate &lt; TODAY()</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Close Date in past?</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
